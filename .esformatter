{
    "plugins": [
        "esformatter-jsx"
    ],
    "jsx": {
        "formatJSX": true,
        "attrsOnSameLineAsTag": false,
        "maxAttrsOnTag": 1,
        "firstAttributeOnSameLine": true,
        "spaceInJSXExpressionContainers": " ",
        "alignWithFirstAttribute": true,
        "htmlOptions": {
            "brace_style": "collapse",
            "indent_char": " ",
            "indent_size": 4,
            "max_preserve_newlines": 2,
            "preserve_newlines": true
        }
    }
}